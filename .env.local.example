# Penguin Health Lambda Environment Configuration
# Copy this file to .env.local and fill in your values
# DO NOT COMMIT .env.local - it is excluded by .gitignore

# S3 Configuration
BUCKET_NAME=your-s3-bucket-name

# SNS Configuration for Textract
SNS_TOPIC_ARN=arn:aws:sns:region:account-id:topic-name
SNS_ROLE_ARN=arn:aws:iam::account-id:role/role-name

# S3 Folder Paths
TEXTRACT_TO_BE_PROCESSED=textract-to-be-processed/
TEXTRACT_IRP_FOLDER=textract-to-be-processed/irp/
TEXTRACT_PROCESSED=textract-processed/
TEXTRACT_IRP_PROCESSED=textract-processed/irp/
TEXTRACT_PROCESSED_IRP=textract-processed/irp/

# DynamoDB Tables
DYNAMODB_TABLE=your-validation-results-table
DYNAMODB_IRP_TABLE=your-irp-table

# Organization Configuration
ORGANIZATION_ID=your-organization-id

# AWS Bedrock Knowledge Base (for rules-engine-rag)
KNOWLEDGE_BASE_ID=your-knowledge-base-id
